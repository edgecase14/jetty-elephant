<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <typedef name="pgsql_enum" class="org.hibernate.type.EnumType">
        <param name="enumClass">net.coplanar.ents.ProjectState$Sm</param>
    </typedef>
    <typedef name="pgsql_enum2" class="org.hibernate.type.EnumType">
        <param name="enumClass">net.coplanar.ents.ProjectState$SmS2</param>
    </typedef>

    <class name="net.coplanar.ents.ProjectStateImpl"> <!-- table="project_state" abstract="true"> -->
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <component class="net.coplanar.ents.ProjectState" name = "pstate">
            <property name="sm" type="pgsql_enum">
                <column name="sm" not-null="false"/>
            </property>
            <property name="smS2" type="pgsql_enum2">
                <column name="smS2" not-null="false"/>
            </property>
        </component>
    </class>
</hibernate-mapping>
